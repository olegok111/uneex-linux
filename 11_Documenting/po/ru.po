#, c-format
msgid "Think of a number no less than 1 and no more than %u.\n"
msgstr "Загадай число не меньше 1 и не больше %u.\n"

#: ../guess.c:169
#, c-format
msgid "Is the number more than %.*s?"
msgstr "Число больше %.*s?"

#: ../guess.c:174
#, c-format
msgid "Is the number more than %u?"
msgstr "Число больше %u?"

#: ../guess.c:179
msgid "y\n"
msgstr "д\n"

#: ../guess.c:187
#, c-format
msgid "The number must be %.*s\n"
msgstr "Должно быть, число равно %.*s\n"

#: ../guess.c:192
#, c-format
msgid "The number must be %u\n"
msgstr "Должно быть, число равно %u\n"
